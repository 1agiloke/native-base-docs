{
    "title": "NativeBase",
    "plugins": [
        "versions",
        "codetabs",
        "include-codeblock",
        "ace",
        "copy-code-button",
        "edit-link",
        "scripts",
        "navigator"
    ],
    "pluginsConfig": {
      "scripts": {
            "files": [
                "./engine.js"
            ]
        },
        "edit-link": {
            "base": "https://github.com/GeekyAnts/native-base-docs/tree/master",
            "label": "Edit This Page"
        },
        "include-codeblock": {
            "template": "ace",
            "theme": "monokai"
        },
        "versions": {
            "gitbookConfigURL": "http://rawgit.com/GeekyAnts/native-base-docs/allNew/book.json",
            "options": [
                {
                    "value": "https://nativebase.io/docs/v2.0.0/",
                    "text": "latest"
                },
                {
                  "value": "http://nativebase.io/docs/v0.5.0/",
                  "text": "v0.5.0"
                },
                {
                  "value": "http://nativebase.io/docs/v0.5.2/",
                  "text": "v0.5.2"
                }
            ]
        }
    }
}
